{
	"name": "VT MDH Fix",
	"version": "1.0.0",
	"description": "Fixes the infinite loading screen when reloading some Virginia Tech My Digital Hand pages",
	"declarative_net_request" : {
    	"rule_resources" : [{
            "id": "ruleset",
            "path": "rules.json",
			"enabled": true
		}]
	},
	"permissions": [
		"declarativeNetRequest"
	],
	"host_permissions": [
		"https://mdh.cs.vt.edu/*"
	],
	"icons": {
		"256": "vtmdhfix.png"
	},
	"manifest_version": 3
}